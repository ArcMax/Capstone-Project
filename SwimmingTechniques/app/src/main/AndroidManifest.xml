<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.explore.archana.swimmingtechniques">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterailTheme">
        <activity
            android:name=".activity.SwimListActivity"
            android:label="@string/app_name"
            android:theme="@style/MyMaterailTheme.Base">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.SwimDetailActivity"
            android:theme="@style/MyMaterailTheme.Detail"/>
    </application>

</manifest>
